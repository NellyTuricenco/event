{
    "env": {
        "browser": true,
        "es6": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "no-var": 0,
        "prefer-rest-params": 0,
        "linebreak-style": 0,
        "no-undef": 0,
        "prefer-arrow-callback": 0,
        "no-new": 0
    }
}